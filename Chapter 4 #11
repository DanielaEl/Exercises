#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <cmath>
using namespace std;

bool isDivisable;

bool check(int x){ 
    if(x%2!=0){
        isDivisable=false;}
    else if(x%3!=0){
        isDivisable=false;}
    return isDivisable;
}


int main()
{
    vector<int>primes;
    primes[0]=2;
    for(int x=1; x<=100; x++){
        check(x);
        if(isDivisable==false){
            primes.push_back(x);
            cout<<x<<", ";}
        else if(isDivisable==true){
            cout<<' ';}
        else{
            cout<<'\n';} 
    }
}
