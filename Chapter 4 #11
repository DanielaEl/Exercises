#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <cmath>

bool isDivisable;

bool prime_check(std::int x, std::vector<int>primes){ 
    if(x%2==0){
        return true;}
    else if(x%3==0){
        return true;}
    else if(x%5==0){
        return true;}
    return false;
}


int main()
{
    std::vector<int>primes{2, 3, 5};
    for(int x=1; x<=100; x++){
        isDivisable=prime_check(x);
        if(isDivisable==false){
            primes.push_back(x);
            std::cout<<x<<", ";}
        }
    std::cout << "Found prime numbers between 1 and 100: \n";
    for (int nPrime : primes)      //Don't know what this line of code does...
    {
        std::cout << nPrime << " ";
    }
    std::cout << '\n';

    return 0;
}
