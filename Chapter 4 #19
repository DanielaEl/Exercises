#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <cmath>

std::vector<std::string>names;
std::vector<int>scores;

bool dup(std::string cn,std::vector<std::string>&names){
    for(std::string lol: names){
        if(cn==lol){
            return true;}
    }
    return false;
}
    
int main(){
    std::string cn;
    int cs;
    bool bad=false;
    bool Bbad=false;
    std::cout<<"Please enter a name followed by a score. (With a space preferably)\n";
    while(bad==false){
    std::cin>>cn>>cs;
    if(Bbad=dup(cn,names){
        std::cout<<"Woops! Seems like you entered the same name by mistake. Here is the list:\n";
        bad=true;
        }
    else{
        names.push_back(cn);
        scores.push_back(cs);
        std::cout<<cn<<' '<<cs<<'\n';}
    }
    
    for(int size2;size2<=names;size2){
        std::cout<<'['<<names[size2]<<"] = "<<scores[size2]<<'\n';}

        return 0;
}
